
generator client {
  provider = "prisma-client-js"
}

generator client2 {
  provider = "prisma-client-js"
  output = "../build/js-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
}
